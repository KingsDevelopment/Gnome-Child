{
  "name": "gnome-child",
  "version": "2.0.0",
  "description": "Gnome child osrs bot",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "repository": {},
  "author": "Jurien Hamaker <jurien@kings-dev.io>",
  "license": "MIT",
  "scripts": {
    "start": "tsc --skipLibCheck && npm run start:js",
    "start:watch": "nodemon",
    "start:js": "node ./dist/index.js",
    "lint": "tslint ./src/**/*.ts",
    "lint:fix": "tslint ./src/**/*.ts --fix",
    "build": "tsc"
  },
  "engines": {
    "node": ">11.x"
  },
  "dependencies": {
    "discord.js": "github:discordjs/discord.js#master",
    "klasa": "github:dirigeants/klasa#master",
    "mongodb": "^3.2.6",
    "typeorm": "^0.2.17"
  },
  "devDependencies": {
    "@types/node": "^7.0.18",
    "nodemon": "^1.18.4",
    "ts-node": "^3.3.0",
    "tslint": "^5.16.0",
    "typescript": "^2.9.2"
  },
  "keywords": [],
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      "dist/*",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
